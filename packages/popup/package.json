{
	"name": "vue-popup-plus",
	"version": "1.1.2",
	"keywords": [
		"vue",
		"vue3",
		"popup",
		"plugin"
	],
	"type": "module",
	"files": [
		"dist"
	],
	"main": "./dist/vue-popup-plus.umd.js",
	"module": "./dist/vue-popup-plus.js",
	"exports": {
		".": {
			"import": "./dist/vue-popup-plus.js",
			"require": "./dist/vue-popup-plus.umd.js"
		}
	},
	"types": "./dist/vue-popup-plus.d.ts",
	"engines": {
		"node": "^20.19.0 || >=22.12.0"
	},
	"scripts": {
		"dev": "vite",
		"build": "run-p build:types && vite build",
		"build:types": "vue-tsc --build",
		"test:unit": "vitest",
		"build-only": "vite build",
		"lint": "eslint . --fix",
		"format": "prettier --write src/"
	},
	"dependencies": {
		"pinia": "^3.0.3",
		"vue": "^3.5.18"
	},
	"devDependencies": {
		"@prettier/plugin-pug": "^3.4.2",
		"@tsconfig/node22": "^22.0.2",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^22.16.5",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vitest/eslint-plugin": "^1.3.4",
		"@vue/eslint-config-prettier": "^10.2.0",
		"@vue/eslint-config-typescript": "^14.6.0",
		"@vue/language-plugin-pug": "^3.0.6",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.7.0",
		"eslint": "^9.31.0",
		"eslint-plugin-vue": "~10.3.0",
		"jiti": "^2.4.2",
		"jsdom": "^26.1.0",
		"npm-run-all2": "^8.0.4",
		"prettier": "3.6.2",
		"pug": "^3.0.3",
		"stylus": "^0.64.0",
		"typescript": "~5.8.0",
		"vite": "^7.0.6",
		"vite-plugin-css-injected-by-js": "^3.5.2",
		"vite-plugin-dts": "^4.5.4",
		"vite-plugin-vue-devtools": "^8.0.0",
		"vitest": "^3.2.4",
		"vue-tsc": "^3.0.4"
	},
	"license": "MIT"
}

