{
	"$schema": "https://json.schemastore.org/prettierrc",
	"trailingComma": "es5",
	"semi": false,
	"singleQuote": true,
	"printWidth": 80,
	"arrowParens": "always",
	"bracketSameLine": true,
	"endOfLine": "crlf",
	"proseWrap": "preserve",
	"singleAttributePerLine": false,
	"tabWidth": 4,
	"useTabs": true
}
